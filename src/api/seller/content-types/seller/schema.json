    },
    "lastName": {
      "type": "string",
      "maxLength": 50,
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "phone": {
      "type": "string",
      "maxLength": 20
    },
    "active": {
      "type": "boolean",
      "default": true
    }
  }
}
